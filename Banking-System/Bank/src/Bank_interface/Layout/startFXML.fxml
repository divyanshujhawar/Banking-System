<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Bank_interface.StartController">
   <center>
      <VBox alignment="CENTER" fillWidth="false" spacing="70.0">
         <children>
            <Label text="Select Employee type" />
            <HBox alignment="CENTER" spacing="80.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#cashier" text="Cashier" />
                  <Button mnemonicParsing="false" onAction="#manager" text="Manager" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
